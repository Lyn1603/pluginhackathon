{
    "manifest_version": 2,
    "name": "RXtool",
    "description": "My Extension ",
    "version": "1.0",
 
    "browser_action": {
       "default_icon": "img/icon.png",
       "default_popup": "index.html"
    },
 
    "icons": {
       "128": "img/icon.png"
    },
    "content_scripts": [
        {
          "matches": ["*://*.rxglobal.fr/fr*"],
          "js": ["js/script.js"],
          "css": ["css/style.css"]
        }
      ]

    

 } 
 